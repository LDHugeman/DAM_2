<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Intents implicitos"/>

    <Button
        android:id="@+id/boton_contactos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="Contactos"/>

    <Button
        android:id="@+id/boton_mostrarTeclado"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="Mostrar teclado "/>

    <Button
        android:id="@+id/boton_marcarNumero"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="Marcar número"/>

    <Button
        android:id="@+id/boton_llamarTelefono"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="Llamar por teléfono"/>

    <Button
        android:id="@+id/boton_abrirNavegador"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="Abrir navegador"/>

    <Button
        android:id="@+id/boton_verMapa"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="Ver mapa teis"/>

</LinearLayout>